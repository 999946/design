{
  "name": "next",
  "version": "0.1.0",
  "description": "next-designer",
  "main": "page/index.tsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-before": "npm run tsc && npm run dll && npm run manage -- --router",
    "start-before-production": "npm run tsc && npm run dll-production",
    "start": "npm run gulp & npm run client-build",
    "tsc": "rm -rf built && tsc",
    "dll": "webpack --config built/scripts/webpack/webpack.dll.config.js",
    "dll-production": "webpack --config built/scripts/webpack/webpack.dll.production.config.js",
    "client-build": "node built/scripts/webpack/webpack-server.js",
    "client-build-production": "webpack --config built/scripts/webpack/webpack.production.config.js --production",
    "server": "node built/server/main.js",
    "server-production": "node built/server/main.js --production",
    "gulp": "gulp --gulpfile built/scripts/gulp/gulpfile.js --cwd ./",
    "gulp-production": "gulp production --gulpfile built/scripts/gulp/gulpfile.js --cwd ./",
    "clean-watch": "sh scripts/clean.sh",
    "manage": "node built/scripts/manage/index.js",
    "production": "rm -rf built-production && PRODUCTION=1 npm run start-before-production && PRODUCTION=1 npm run gulp-production && PRODUCTION=1 npm run client-build-production",
    "production-local": "rm -rf built-production && npm run start-before-production && npm run gulp-production && npm run client-build-production",
    "clean": "pkill node; pkill gulp; pkill built/server/main.js; pkill built/scripts/webpack/webpack-server.js; pgrep node;"
  },
  "repository": {
    "type": "git",
